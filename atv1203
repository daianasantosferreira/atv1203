Inicio
// Seção de Comandos, procedimento, funções, operadores, etc...
SE(media < 5)entao
  resultado <- "i"
 SENAO
   SE(media <7>)entao
     resultado <- "R"
   SENAO
      SE (media < 9> entao
          resultado <- "MB"
   fimse
   fimse
fimse
escreva("a sua menção final foi",resultado)
 

